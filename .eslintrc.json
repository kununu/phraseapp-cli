{
"rules": {
  "semi": [
  2, "always"
  ],
"curly": [
  2, "multi-line"
],
"one-var": [
  2, {
    "var": "always",
    "let": "never",
    "const": "never"
  }
],
"import-order/import-order": 2,
"guard-for-in": 2,
"object-shorthand": 2,
"no-path-concat": 2,

"no-use-before-define": [
"error", {
  "functions": true,
  "classes": true
}],

"quote-props": [
  2, "as-needed"
],
"space-before-function-paren": [
  2, "never"
],
"prettier/prettier": [
  "error", {
  "singleQuote": true,
  "bracketSpacing": false,
  "jsxBracketSameLine": true,
  "parser": "flow"
}]
},
"env": {
  "es6": true,
  "node": true
},
"plugins": [
  "import-order",
  "prettier"
],
"extends": "standard"
}